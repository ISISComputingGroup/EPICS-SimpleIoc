record(bo, "$(P)SIM")
{
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
    field(ZNAM, "NO")
    field(ONAM, "YES")
    info("testparam","testvalue")
}

record(bo, "$(P)DISABLE") 
{
    field(DESC, "Disable comms")
    field(PINI, "YES")
    field(VAL, "0")
    field(OMSL, "supervisory")
    field(ZNAM, "COMMS ENABLED")
    field(ONAM, "COMMS DISABLED")
}

record(ai, "$(P)VALUE1")
{
    info(INTEREST, "HIGH")
	field(DESC, "Testing")
    field(INP, "$(P)VALUE1:SP")
    field(HIHI,18)
    field(HIGH,15)
    field(LOW,5)
    field(LOLO,2)
    field(HHSV,"MAJOR")
    field(HSV,"MINOR")
    field(LSV,"MINOR")
    field(LLSV,"MAJOR")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:VALUE1")
    field(SDIS, "$(P)DISABLE")
	field(EGU, "")
}

record(ao, "$(P)VALUE1:SP")
{
    field(VAL, 1)
    field(HIHI,18)
    field(HIGH,15)
    field(LOW,5)
    field(LOLO,2)
    field(HHSV,"MAJOR")
    field(HSV,"MINOR")
    field(LSV,"MINOR")
    field(LLSV,"MAJOR")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:VALUE1")
    field(SDIS, "$(P)DISABLE")
    field(FLNK, "$(P)VALUE1")
}

alias("$(P)VALUE1", "$(P)VALUE1:SP:RBV")

