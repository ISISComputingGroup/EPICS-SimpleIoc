record(calc,"$(P)DIFF")
{
    field(SCAN,"1 second")
    field(INPA, "$(P)VALUE1")
    field(INPB, "$(P)VALUE2")
    field("CALC", "A - B")
}

record(ai,"$(P)RANDVAL"){
    field(DTYP,"Random")
    field(DESC,"Random numbers")
    field(SCAN,"1 second")
    field(INP,"324235")
    field(LINR,"LINEAR")
    field(ESLO,1)
    field(EOFF,0)
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:RANDVAL")
    field(SDIS, "$(P)DISABLE")
}

record(stringin,"$(P)HELLO"){
    info(INTEREST, "HIGH")
	field(DESC,"A stringin record")
    field(VAL, "Hello!")
}

record(waveform, "$(P)CHARWAV")
{
    field(DESC, "A char waveform record")
    field(NELM, "8192")
    field(FTVL, "CHAR")
}

